nents":[{"tagName":"span","type":"text","name":"Card Title","classes":["card-title"],"components":[{"type":"textnode","content":"Advanced"}]},{"tagName":"span","type":"text","name":"Card Subtitle","classes":["card-subtitle"],"components":[{"type":"textnode","content":"Complex and fast"}]}]}]},{"tagName":"p","type":"text","name":"Card Description","classes":["card-description"],"components":[{"type":"textnode","content":"Technical vocabulary, symbols, and speed-focused prompts."}]},{"name":"Card CTA","classes":["group-hover:text-indigo-200","card-cta"],"components":[{"type":"image","name":"Card CTA Icon","resizable":{"ratioDefault":1},"classes":["card-cta-icon"],"attributes":{"alt":"Arrow Icon","src":"https://api.iconify.design/lucide-arrow-right.svg?color=%23a5b4fc"}},{"tagName":"span","type":"text","name":"Card CTA Text","classes":["card-cta-text"],"components":[{"type":"textnode","content":"Select Advanced"}]}]}]}]}]}]}]},{"tagName":"section","name":"Typing Section","classes":["typing-section"],"components":[{"name":"Typing Inner","classes":["typing-inner"],"components":[{"name":"Content Grid","classes":["content-grid"],"components":[{"name":"Left Column","classes":["left-column"],"components":[{"name":"Prompt Card","classes":["gjs-t-border","prompt-card"],"attributes":{"id":"i2cvj"},"components":[{"name":"Prompt Header","classes":["prompt-header"],"attributes":{"id":"i0pp6"},"components":[{"name":"Prompt Title Group","classes":["prompt-title-group"],"components":[{"type":"image","name":"Prompt Icon","resizable":{"ratioDefault":1},"classes":["prompt-icon"],"attributes":{"alt":"Text Icon","src":"https://api.iconify.design/lucide-text-cursor-input.svg?color=%23e5e7eb"}},{"tagName":"span","type":"text","name":"Prompt Label","classes":["prompt-label"],"components":[{"type":"textnode","content":"Prompt"}]}]},{"name":"Prompt Actions","classes":["prompt-actions"],"components":[{"type":"button","name":"Prompt Action Button","classes":["gjs-t-border","prompt-action-button"],"attributes":{"type":"button","id":"btn-new-prompt"},"components":[{"type":"contentEditable","components":[{"type":"textnode","content":"New Prompt"}]}]},{"type":"button","name":"Prompt Action Button","classes":["gjs-t-border","prompt-action-button"],"attributes":{"type":"button","id":"btn-reset"},"components":[{"type":"contentEditable","components":[{"type":"textnode","content":"Reset"}]}]}]}]},{"tagName":"p","type":"text","name":"Prompt Text","classes":["prompt-text"],"attributes":{"id":"prompt-text"},"components":[{"type":"textnode","content":"On a rainy morning, a curious commuter noticed a hand‑painted sign outside a small shop: “Repairs, Restorations, and Stories.” Inside, clocks of every shape ticked in gentle, overlapping rhythms—grandfather clocks with polished brass pendulums, pocket watches with engraved cases, and modern timepieces humming quietly behind glass. The watchmaker, an attentive man with ink‑stained fingers, greeted the commuter with a nod and slid a tray of tiny gears across the counter. “Time,” he said, “is a chorus. Every instrument has to play its part—steady, precise, and in tune.” He demonstrated the careful alignment of a balance wheel, the delicate tension of a spring, and the satisfying click when a tooth met its matching notch. As the commuter leaned closer, the watchmaker continued: “People chase speed, but accuracy is what makes progress repeatable. If a second goes missing here or there, the whole day drifts.” Outside, buses hissed, umbrellas knocked, and shoes tapped over puddled bricks; inside, the metronomic pulse kept calm order. The commuter realized that practice resembled the watchmaker’s craft: consistent technique, mindful pacing, and a willingness to pause, correct, and continue. “When you type,” the watchmaker added, “listen for your rhythm—keys should feel like small gears meshing, neither forced nor hesitant.” The commuter thanked him, stepped back into the weather, and, with renewed focus, began to move through the morning as though each minute were a well‑tuned note in a resilient, hopeful song."}]}]},{"name":"Typing Box Card","classes":["gjs-t-border","typing-box-card","typing-area-prominent"],"attributes":{"id":"i5e6od"},"components":[{"type":"label","classes":["typing-input-label"],"attributes":{"for":"typing-input","id":"is0mju"},"components":[{"type":"textnode","content":"Typing input area"}]},{"type":"textarea","classes":["typing-input"],"attributes":{"id":"typing-input","rows":"16","placeholder":"Start typing the prompt here...","aria-describedby":"ioj8j3"}},{"classes":["typing-actions-row"],"attributes":{"id":"iqh9j9"},"components":[{"name":"Typing Hint","classes":["typing-hint"],"attributes":{"id":"iq5wsr"},"components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["typing-hint-icon"],"attributes":{"alt":"Info Icon","src":"https://api.iconify.design/lucide-info.svg?color=%23cbd5e1","id":"ir5cro"}},{"tagName":"span","type":"text","classes":["typing-hint-text"],"attributes":{"id":"ioj8j3"},"components":[{"type":"textnode","content":"Press Enter to submit when you finish."}]}]},{"type":"button","classes":["gjs-t-button","complete-button"],"attributes":{"type":"button","id":"btn-complete"},"components":[{"type":"contentEditable","components":[{"type":"textnode","content":"Complete Test"}]}]}]}]}]},{"tagName":"aside","name":"Right Column","classes":["right-column"],"components":[{"name":"Metrics Card","classes":["gjs-t-border","metrics-card"],"components":[{"name":"Metrics Header","classes":["metrics-header"],"components":[{"name":"Metrics Title Group","classes":["metrics-title-group"],"components":[{"type":"image","name":"Metrics Icon","resizable":{"ratioDefault":1},"classes":["metrics-icon"],"attributes":{"alt":"Speed Icon","src":"https://api.iconify.design/lucide-speedometer.svg?color=%23e5e7eb"}},{"tagName":"span","type":"text","name":"Metrics Title","classes":["metrics-title"],"components":[{"type":"textnode","content":"Real-time Metrics"}]}]},{"tagName